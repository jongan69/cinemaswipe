{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "noImplicitAny": false,
    "typeRoots": ["./node_modules/@types"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "***/components/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
