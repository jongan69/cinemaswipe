{
  "expo": {
    "name": "cinema-swipe",
    "slug": "cinema-swipe",
    "version": "1.0.0",
    "description": "A movie match maker",
    "orientation": "portrait",
    "owner": "jongan69",
    "githubUrl": "https://github.com/jongan69/cinemaswipe",
    "icon": "./assets/images/icon.png",
    "scheme": "cinema-swipe",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jongan69.cinemaswipe"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jongan69.cinemaswipe"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "babel": {
        "include": [
          "@ui-kitten/components",
          "@ui-kitten/eva-icons",
          "@aws-amplify/ui-react-native"
        ]
      }
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bf8470d0-802b-4fc4-8768-ea749c83adc2"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/bf8470d0-802b-4fc4-8768-ea749c83adc2"
    }
  }
}
